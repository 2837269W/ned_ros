<?xml version="1.0"?>
<?xml-model
  href="http://download.ros.org/schema/package_format3.xsd"
  schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>niryo_robot_rpi</name>
  <version>3.0.0</version>
  <description>Specific utilities for Ned RPI + custom Ned shield</description>
  <maintainer email="v.pitre@niryo.com">Valentin Pitre</maintainer>
  <license>GPLv3</license>
  <author>Edouard Renard</author>
  <author email="v.pitre@niryo.com">Valentin Pitre</author>
  
  <buildtool_depend>catkin</buildtool_depend>
  
  <build_depend>rospy</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>niryo_robot_msgs</build_depend>

  <exec_depend>rospy</exec_depend>
  <exec_depend>std_msgs</exec_depend>
  <exec_depend>niryo_robot_msgs</exec_depend>

  <!-- Python Dependencies -->
  <exec_depend>python-flask</exec_depend>
  <exec_depend>python-rpi.gpio</exec_depend>
  
</package>
