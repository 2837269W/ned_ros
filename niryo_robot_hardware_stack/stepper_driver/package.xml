<?xml version="1.0"?>
<package format="2">
  <name>stepper_driver</name>
  <version>0.0.0</version>
  <description>The stepper_driver package provides an interface bewteen high level ROS packages and stepper motors</description>
  <author>Etienne Rey-Coquais</author>
  <author>Edouard Renard</author>
  <maintainer email="e.rey-coquais@niryo.com">Etienne Rey-Coquais</maintainer>

  <license>GPLv3</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roscpp</build_depend>
  <build_depend>mcp_can_rpi</build_depend>
  <build_depend>niryo_robot_msgs</build_depend>
  <build_depend>message_generation</build_depend>
  <build_depend>common</build_depend>

  <exec_depend>roscpp</exec_depend>
  <exec_depend>mcp_can_rpi</exec_depend>
  <exec_depend>niryo_robot_msgs</exec_depend>
  <exec_depend>common</exec_depend>

</package>


