<?xml version="1.0" ?>
<launch xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://gist.githubusercontent.com/nalt/dfa2abc9d2e3ae4feb82ca5608090387/raw/roslaunch.xsd">
  <!-- Arguments -->
  <arg name="hardware_version" default="ned"/>

  <rosparam file="$(find ttl_driver)/config/$(arg hardware_version)/dynamixels_params.yaml" command="load"/>

  <group ns="ttl_driver">  
    <rosparam file="$(find ttl_driver)/config/$(arg hardware_version)/bus_config.yaml" command="load"/>
    <rosparam file="$(find ttl_driver)/config/$(arg hardware_version)/motors_config.yaml" command="load"/>
    <rosparam file="$(find ttl_driver)/config/$(arg hardware_version)/led.yaml" command="load"/>
    <rosparam file="$(find ttl_driver)/config/default.yaml" command="load"/>
  </group>
</launch>
