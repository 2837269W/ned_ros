<?xml version="1.0"?>
<package format="2">
  <name>ttl_driver</name>
  <version>0.0.0</version>
  <description>The ttl_driver package provides an interface bewteen high level ROS packages and motors on TTL bus</description>
  <author>Etienne Rey-Coquais</author>
  <author>Edouard Renard</author>
  <author>Valentin Pitre</author>
  <author>Clement Cocquempot</author>
  <maintainer email="c.cocquempot@niryo.com">Clement Cocquempot</maintainer>

  <license>GPLv3</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roscpp</build_depend>
  <build_depend>dynamixel_sdk</build_depend>
  <build_depend>niryo_robot_msgs</build_depend>
  <build_depend>common</build_depend>

  <exec_depend>roscpp</exec_depend>
  <exec_depend>dynamixel_sdk</exec_depend>
  <exec_depend>niryo_robot_msgs</exec_depend>
  <exec_depend>common</exec_depend>
</package>
