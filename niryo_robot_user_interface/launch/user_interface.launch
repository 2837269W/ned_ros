<?xml version="1.0" ?>
<launch xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://gist.githubusercontent.com/nalt/dfa2abc9d2e3ae4feb82ca5608090387/raw/roslaunch.xsd">
    <!-- Arguments -->
    <arg name="log_level" default="INFO" />

    <group ns="niryo_robot">
        <group ns="python_ros_wrapper">
            <param name="service_timeout" type="int" value="2"/>
            <param name="action_connection_timeout" type="int" value="20"/>
            <param name="action_execute_timeout" type="int" value="60"/>
            <param name="action_preempt_timeout" type="int" value="60"/>
        </group>
    </group>

    <!--  Launch Main node  -->
    <node name="niryo_robot_user_interface" pkg="niryo_robot_user_interface" type="user_interface_node.py" output="screen" >
        <param name="log_level" type="string" value="$(arg log_level)" />
    </node>
</launch>
