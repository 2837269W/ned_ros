<package>
  <name>niryo_robot_python_ros_wrapper</name>
  <version>3.1.1</version>
  <description>Python ROS Wrapper to get Niryo Robot ROS functionalities, without knowing ROS</description>
  <author email="r.lux@niryo.com">Remi Lux</author>
  <maintainer email="r.lux@niryo.com">Remi Lux</maintainer>
  <maintainer email="c.ducatez@niryo.com">Corentin Ducatez</maintainer>
  <license>GPLv3</license>
  
  <buildtool_depend>catkin</buildtool_depend>

  <run_depend>actionlib_msgs</run_depend>
  <run_depend>geometry_msgs</run_depend>
  <run_depend>niryo_robot_commander</run_depend>
  <run_depend>niryo_robot_msgs</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>std_msgs</run_depend>

</package>
